$("#result").remove();
$("body").append("<%= j(render('result')) %>");
<% if @detected_items.present? %>
  $("#result").append("<%= j(render('judged_items', judged_items: @judged_items)) %>");
  $('#result').append("<%= j(render('unregd_items', unregd_items: @unregd_items)) %>");
<% else %>
  document.querySelector(".judge").innerHTML = '<p>何も検出できませんでした、、、。<br>（写真を鮮明にしてみると解決するかもしれません）<p>';
<% end %>